<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>local.eventreminder.automator</string>
    <key>KeepAlive</key>
    <false/>
    <key>RunAtLoad</key>
    <false/>
    <key>UserName</key>
    <string>zizhensong</string>
    <key>Program</key>
    <string>/PATH/TO/autoemailer.sh</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.user.imagecopier.stderr</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.user.imagecopier.stdout</string>
    <key>WorkingDirectory</key>
    <string>/PATH/TO/WORKINGDIRECTORY/</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>05</integer>
        <key>Minute</key>
        <integer>00</integer>
    </dict>
</dict>
</plist>